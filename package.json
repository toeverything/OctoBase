{
	"name": "blog",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "turbo --filter @toeverything/jwt* build && pnpm --filter @toeverything/playground dev",
		"dev:homepage": "turbo --filter @toeverything/jwt* build && pnpm --filter @toeverything/octobase-homepage dev",
		"build": "turbo build",
		"build:playground": "pnpm --filter @toeverything/jwt* --filter @toeverything/playground build"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"react",
				"react-dom",
				"rollup",
				"vite"
			]
		}
	},
	"dependencies": {
		"prettier": "^2.8.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.8.2"
	},
	"devDependencies": {
		"@rollup/plugin-replace": "^5.0.2",
		"@types/node": "^18.14.6",
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"rollup-plugin-typescript2": "^0.34.1",
		"rollup-plugin-visualizer": "^5.9.0",
		"terser": "^5.16.5",
		"turbo": "^1.8.3",
		"typescript": "^4.9.5",
		"vite": "^4.1.4"
	}
}
